{
  "name": "@loli-works/snbt",
  "version": "1.0.0",
  "description": "A library for parsing SNBT (Stringified NBT) format",
  "scripts": {
    "gen-parser": "ts-node ./build-parser.mjs",
    "gen-highlighter": "ts-node ./build-highlighter.mjs",
    "test": "jest --passWithNoTests",
    "minify": "rollup -c && uglifyjs ./dist/snbt.js -o ./dist/snbt.min.js --compress --mangle",
    "build": "pnpm run gen-parser && pnpm run gen-highlighter && tsc && pnpm run minify"
  },
  "keywords": [
    "SNBT",
    "Parser",
    "Grammar"
  ],
  "author": "Nickid2018",
  "license": "MIT",
  "files": [
    "dist/snbt.js",
    "dist/snbt.js.map",
    "dist/snbt.min.js",
    "dist/snbt.d.ts"
  ],
  "types": "dist/snbt.d.ts",
  "main": "dist/snbt.js",
  "packageManager": "pnpm@10.5.0",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "jest": "^29.7.0",
    "peggy": "^4.2.0",
    "prettier": "^3.4.2",
    "rollup": "^4.40.0",
    "rollup-plugin-dts": "^6.2.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "uglify-js": "^3.19.3"
  }
}
